{
  "expo": {
    "name": "Sognudimare",
    "slug": "sognudimare",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sognudimare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sognudimare.app",
      "buildNumber": "2",
      "infoPlist": {
        "NSCameraUsageDescription": "Prendre des photos pour partager vos moments en mer",
        "NSPhotoLibraryUsageDescription": "Partager vos photos de croisi√®re"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0e1c40"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.sognudimare.app",
      "versionCode": 2
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0e1c40"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "sognudimare-app"
      },
      "backendUrl": "https://sognudimare-app-production.up.railway.app"
    }
  }
}
