{
  "expo": {
    "name": "Sognudimare",
    "slug": "sognudimare",
    "version": "1.7.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "sognudimare",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0e1c40"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sognudimare.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#0e1c40"
      },
      "package": "com.sognudimare.app"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "eas": {
        "projectId": "b7a9c1d8-ab6b-4ae4-8b4e-3f0cde55d8f0"
      }
    }
  }
}
